<main class="container">
  <%= link_to sanitize("&larr; Back"), experience_entry_date_path(@experience, @entry_date) %>

  <div class="header">
    <h1>New Event</h1>
  </div>

  <%= turbo_frame_tag @event do %>
    <%= render "form", experience: @experience, entry_date: @entry_date, event: @event %>
  <% end %>
</main>